{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "no-useless-constructor": [0],
    "eol-last": [0],
    "no-trailing-spaces": [0],
    "react/prefer-stateless-function": [0],
    "import/no-extraneous-dependencies": [0],
    "react/jsx-filename-extension": [0],
    "import/first": [0],
    "comma-dangle": [0],
    "react/self-closing-comp": [0],
    "spaced-comment": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "function-paren-newline": [0],
    "import/extensions": [0],
    "quotes": [0],
    "no-undef": [0],
    "no-plusplus": [0],
    "no-console": [0],
    "space-unary-ops": [0],
    "no-unused-vars": [0],
    "react/forbid-prop-types": [0],
    "prefer-const": [0],
    "padded-blocks": [0],
    "arrow-parens": [0]
  }
}
